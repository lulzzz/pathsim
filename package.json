{
  "name": "pathsim",
  "version": "1.0.0",
  "description": "path algorithm simulator",
  "main": "index.html",
  "private": true,
  "dependencies": {
    "angular": "1.5.3",
    "js-priority-queue": "^0.1.5",
    "lodash": "^4.7.0",
    "systemjs": "^0.19.38"
  },
  "devDependencies": {    
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "tape": "^4.5.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"    
  },
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "test": "node_modules/tape/bin/tape  'tests/**/*.js'",
    "watch-test": "watch \"node_modules/tape/bin/tape 'tests/**/*.js'\" tests/",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "author": "Oliver Guhr",
  "license": "MIT"
}
